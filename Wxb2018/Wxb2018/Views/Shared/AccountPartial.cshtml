<!-- 登录弹窗 -->
<div class="denglu-box center-1" id="login-Div">
    <div class="enter-2">
        <div class="title-close">登录<a class="closeBtn"></a></div>
        <form>
            <div class="error" id="lg-error">密码输入错误，请重新输入。</div>
            <ul>
                <li><input id="lg-userName" autocomplete="off" class="text-2" name="textfield" type="text" placeholder="请输入用户名" value="" /></li>
                <li><input id="lg-password" type="password" placeholder="请输入密码" class="password-2" /></li>
                <li class="ckbox" style="display:none;"><label><input id="lg-rememberMe" type="checkbox" /><em>记住密码</em></label></li>
            </ul>
        </form>
    </div>
</div>

